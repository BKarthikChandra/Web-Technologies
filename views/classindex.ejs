<!DOCTYPE html>
<html lang="en" dir="ltr">
  <head>
    <meta charset="utf-8">
    <title></title>

    <style>
      body,html
      {
         padding: 0px;
         margin:0px;
         height: 100%;
         width: 100%;
         background-color: #F5F5F5;
        
      }
      .b1 {
       height: 100%;
       width: 100%;
       background-color: #4267B2;
       overflow: hidden;
}
    .b1  ol li
    {

       display: inline-block;
    }
    /* .b1 nav
    {
      float: right;
    } */
    nav {
      background-color: #4267B2;
      overflow: hidden;
      height: 100%;
      float: right;

    }

    nav a {
      
      display: inline-block;
      color: white;
      text-align: center;
      padding: 2px 16px;
      text-decoration: none;
    }

    nav a:hover {
      background-color: #ddd;
      color: black;
    }
       .b4
       {
        width:100%;
        height:10%;
        margin: 0px;
       }
       .b5
       {
        width:100%;
        height:85%;
       }
       .b2
       {
        padding-top: 10px;
       }
       .b2 tr,td
       {
        padding:10px;
       }
       .ex{
        margin-left: 430px;
       }
       table 
       {
        margin:auto;
       }
       
       .ex2{
        margin-left: 50px;
        text-align: center;
       }
       .b1 img {
  position: absolute;
  top: 0;
  left: 0;
  display: inline;
  height: 65px;
  width: 100px;
}
    </style>
  </head>
  <body>
     
     
         <div class = "b4">
          <div class = "b1">
            <img src="/OIP" alt="">
            <nav>
              <ol>
                <li><a   onclick = "f()" >View Time Table</a></li>
          
               
               
                  <li><a href="/logout">Logout</a></li>
              </ol>
            </nav>
       </div>
         </div>
         <div class = "b2">
          <h1  class="ex2">Timetable for <%= name %></h1>
      <table border = 1 style = "border-collapse:collapse">
        <thead>
          <tr>
            <th>Day</th>
            <th>8:40 - 9:30</th>
            <th>9:30 - 10:20</th>
            <th>10:20 - 11:10</th>
            <th>11:10 - 12:00</th>
            <th>12:00 - 12:50</th>
            <th>12:50 - 01:40</th>
            <th>01:40 - 02:30</th>
            <th>02:30 - 03:20</th>
            <th>03:20 - 04:10</th>
          </tr>
        </thead>
        <tbody>
          <% for (let i = 0; i < results.length; i++) { %>
            <tr>
              <td><%=  results[i].Day %></td>
              <td><%=  results[i].Period1 %></td>
              <td><%=  results[i].Period2 %></td>
              <td><%=   results[i].Period3 %></td>
              <td><%=   results[i].Period4 %></td>
              <td>Lunch Break</td>
              <td><%=  results[i].Period5 %></td>
              <td><%=  results[i].Period6 %></td>
              <td><%=   results[i].Period7 %></td>
              <td><%=  results[i].Period8 %></td>
            </tr>
          <% } %>
        </tbody>
      </table>
       </div>     
        
    
  </body>
</html>